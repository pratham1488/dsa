#include <iostream>
using namespace std;
int main() {
    int size ;
    cout<<"Enter size and elements of array";
    cin>>size;
    int array[size];
    for(int i = 0; i < size; i++) {
        cin>>array[i];
    }
    for( int i = 0; i < size ; i++) {
        for(int j = i + 1 ; j < size; j++) {
            for(int k = 0 ; k < size; k++) {
                if(array[i] + array[j] == array[k]){
                    cout<<array[i]<<","<<array[j]<<","<<array[k]<<endl;
                }
            }    
        }
    }
}
